{
	"name": "lutheran/meet",
	"description": "MEET",
	"minimum-stability": "stable",
	"license": "proprietary",
	"require": {
		"php": ">=8.2.0",
		"yiisoft/yii2": "~2.0.49",
		"yiisoft/yii2-bootstrap": "~2.0.0",
		"yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
		"yiisoft/yii2-httpclient": "^2.0.0",
		"phpoffice/phpspreadsheet": "^2.0.0",
		"yiisoft/yii2-bootstrap4": "^2.0.11",
		"tijsverkoyen/css-to-inline-styles": "^2.2.7",
		"google/recaptcha": "^1.3.0",
		"knplabs/knp-snappy": "^v1.5.0",
		"ext-pdo": "*",
		"pontedilana/php-weasyprint": "^1.4"
	},
	"require-dev": {
		"yiisoft/yii2-debug": "^2.1.25",
		"yiisoft/yii2-gii": "~2.2.6",
		"yiisoft/yii2-faker": "~2.0.0",
		"codeception/codeception": "^5.0.13",
		"codeception/verify": "^3.0.0",
		"codeception/specify": "^2.0.0",
		"symfony/browser-kit": "^6.0.0",
		"codeception/module-filesystem": "^3.0.0",
		"codeception/module-yii2": "^1.1.10",
		"codeception/module-asserts": "^3.0.0",
		"squizlabs/php_codesniffer": "^3.8.1",
		"srggroup/srg_standards": "dev-master"
	},
	"scripts": {
		"post-install-cmd": [
			"yii\\composer\\Installer::postInstall"
		],
		"post-create-project-cmd": [
			"yii\\composer\\Installer::postCreateProject",
			"yii\\composer\\Installer::postInstall"
		]
	},
	"extra": {
		"yii\\composer\\Installer::postCreateProject": {
			"setPermission": [
				{
					"runtime": "0777",
					"web/assets": "0777",
					"yii": "0755"
				}
			]
		},
		"yii\\composer\\Installer::postInstall": {
			"generateCookieValidationKey": [
				"config/web.php"
			]
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		},
		{
			"type": "vcs",
			"url": "https://github.com/srggroup/srg_standards"
		}
	],
	"autoload": {
		"files": [
			"lib/global_functions.php"
		]
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	}
}
